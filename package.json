  "prettier": "prettier-config-rive",
  "eslintConfig": {
    "extends": "eslint-config-rive/react"
  },
  "stylelint": {
    "extends": "stylelint-config-rive"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "rc-util-modern": "^5.39.1"
  },
  "devDependencies": {
    "@mdx-js/react": "^3.0.1",
    "@rc-component/father-plugin": "^1.0.2",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^15.0.5",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.12.7",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@umijs/fabric": "^2.14.1",
    "cross-env": "^7.0.3",
    "father": "^4.4.0",
    "gh-pages": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "rc-test": "^7.0.15",
    "react": "^18.3.1",
    "react-doc-ui": "^2.2.6",
    "react-dom": "^18.3.1",
    "rive": "^2.0.33",
    "typescript": "^5.4.5",
    "whatwg-fetch": "^3.6.20"
