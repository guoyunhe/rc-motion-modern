{
  "name": "rc-motion-modern",
  "version": "2.9.1",
  "description": "React lifecycle controlled motion library (for Chrome 86+)",
  "keywords": [
    "react",
    "react-component",
    "react-motion",
    "motion",
    "antd",
    "ant-design"
  ],
  "homepage": "https://github.com/guoyunhe/rc-motion-modern",
  "bugs": {
    "url": "https://github.com/guoyunhe/rc-motion-modern/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:guoyunhe/rc-motion-modern.git"
  },
  "funding": "https://github.com/sponsors/guoyunhe",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rive build",
    "build:watch": "rive build --watch",
    "lint": "rive lint",
